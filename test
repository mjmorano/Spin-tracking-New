g++  -g -w -O3 -std=c++17 -fPIC -fopenmp    -c src/double3.cpp -o build/double3.o 
g++  -g -w -O3 -std=c++17 -fPIC -fopenmp    -c src/optionsParser.cpp -o build/optionsParser.o 
g++  -g -w -O3 -std=c++17 -fPIC -fopenmp    -c src/integrator.cpp -o build/integrator.o 
g++  -g -w -O3 -std=c++17 -fPIC -fopenmp    -c src/particle.cpp -o build/particle.o 
g++  -g -w -O3 -std=c++17 -fPIC -fopenmp    -c src/simulation.cpp -o build/simulation.o 
